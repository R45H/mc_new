mixin rate(data)
	.rate

		.rate__item
			.rate__title Количество участников
			.title.title_color_main.title_lower= data.count + ' человек'

		each val in data.items
			.rate__item
				.rate__title= val.title
				.rate__star-wrap

					- for (var i = 1; i <= 5; i++) {
						.star(class= i > val.rate ? 'star_disabled' : '')
					- }
				.title.title_sm.title_color_main= val.rate + ' / 5'