mixin rate(data)
	.rate

		each val in data
			.rate__item
				.rate__title= val.title

				if val.rate
					.rate__star-wrap

						- for (var i = 1; i <= 5; i++) {
							.star(class= i > val.rate ? 'star_disabled' : '')
						- }
					.title.title_sm.title_color_main.title_lower= val.value

				else
					.title.title_color_main.title_lower= val.value